{% embed "@templates/_default.twig" with {
  has_sidebar: true,
} %}
  {% block section_header %}
    {% include "@organisms/site/site-header/site-header.twig" %}
  {% endblock %}
  {% block section_content %}
    {% include "@templates/placeholder/place-holder.twig" with {
      "place_holder": "Content",
    } %}
  {% endblock %}
  {% block section_sidebar %}
    {% include "@templates/placeholder/place-holder.twig" with {
      "place_holder": "Sidebar",
    } %}
  {% endblock %}
  {% block section_footer %}
    {% include "@organisms/site/site-footer/site-footer.twig" %}
  {% endblock %}
{% endembed %}
