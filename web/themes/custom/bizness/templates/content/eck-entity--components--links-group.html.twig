/**
 * @file
 * Overwrite from web/modules/contrib/eck/templates/eck-entity.html.twig
 */
#}

{% set link_list_primary__items = [] %}

{% for key, item in content.field_components_ref_unlimited %}
  {% if key matches '/^\\d+$/' %}
    {% set link_list_primary__items = link_list_primary__items|merge([{
      link_content: item["#components"].field_link.value[0].title,
      link_url: item["#components"].field_link.value[0].uri,
      link_description: item["#components"].field_description_plain_long.value
    }]) %}
  {% endif %}
{% endfor %}

{% embed "@molecules/links-list/primary/link-list-primary.twig" with {
  link_list_primary__heading: content.field_text[0],
  link_list_primary__items: link_list_primary__items,
} %}
{% endembed %}
